    "date-fns": "^4.1.0",
    "dompurify": "^3.0.8",
    "i18next": "^25.8.0",
    "lucide-react": "^0.475.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^16.5.4",
    "react-router-dom": "^7.1.3",
    "react-window": "^1.8.11",
    "recharts": "^2.15.1",
